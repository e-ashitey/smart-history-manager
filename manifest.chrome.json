{
    "manifest_version": 3,
    "name": "Smart History Manager",
    "version": "1.0",
    "description": "Search, group, and clean your browsing history by URL or keyword.",
    "permissions": [
        "history",
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "service_worker.js"
    },
    "action": {
        "default_popup": "popup/popup.html",
        "default_title": "Smart History Manager"
    }
}